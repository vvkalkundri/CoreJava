package com.java.innerclass;

public class AnnoymousInnerClass {
         PopCorn p = new PopCorn();
         p.Pop();
         
}

class PopCorn{
	public void Pop(){
		System.out.println(" This is pop of popcorn class");
	}
}

class Food{
	PopCorn p = new PopCorn(){
		public void Pop(){
			System.out.println("This is pop of  Food class");
		}
	};
		
}