package com.java.test;

public class funnyProductTest {

	public static void main(String[] args) {
		 Boo b = new Boo();
		 
		}
	}

	class Boo{
		Boo() {
			System.out.println( "This is the default constructor of the Boo class");
		}
		Boo(String s){
			System.out.println("This is the 1 arguement constructor : " +s);
		}
	}