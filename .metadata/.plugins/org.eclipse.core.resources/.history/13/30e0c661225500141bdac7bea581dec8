package com.java.test;

class Salman {
	public static void main(String[] args) {
		String valOfI = "";
		String valOfJ = "";
		for (int i = 1; i <= 12; i++) {
			for (int j = 1; j <= 31; j++) {
				if( i < 9){
					 valOfI = "0" + String.valueOf(i);
				}else{
					 valOfI =String.valueOf(i);
				}
				if( j<9){
					valOfJ  = "0" + String.valueOf(j);
				}else{
					valOfJ  =  String.valueOf(j);
				}
				
					System.out.println("2014-" + valOfI + "-"  + valOfJ);
				
					
				}

			}
		}

	}

